[mysqld]

port = 3313
max_connections = 353
innodb_buffer_pool_size = 255M
local-infile=0
symbolic-links=0
log_error = /var/log/mysql/error.log
general_log = 1
general_log_file = /var/log/mysql/mysql-general.log
slow_query_log = 1
slow_query_log_file = /var/log/mysql/mysql-slow.log
log_warnings = 9
long_query_time = 3
expire_logs_days = 24

