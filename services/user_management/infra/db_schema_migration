mkdir -p /app/$APP_LOG_PATH/

python3 manage.py makemigrations user_management  --settings settings.migration --name tables_composite_pk

exit_code=$?

if [ $exit_code -ne 0 ]; then
    exit $exit_code
fi

python3 manage.py migrate user_management  0003  --settings settings.migration  --database site2_dba

exit $?
